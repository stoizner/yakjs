{
    "env": {
        "browser": true,
        "node": true
    },
    "globals": {
        "yak": true,
        "_": true,
        "$": true
    },
    "rules": {
        "eqeqeq": 0,
        "no-use-before-define": 0,
        "no-unused-vars": 0,
        "no-trailing-spaces": 0,
        "no-extra-strict": 0,
        "quotes": [2, "single"],
        "strict": 0,
        "func-names": 2,
        "curly": 2
    }
}
